{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "",
  "main": "dist/module.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "webpack --no-minimize",
    "postbuild": "webpack",
    "coverage": "isparta cover _mocha -- --require babel/register",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test": "mocha --recursive --compilers js:babel/register --reporter nyan",
    "test:browser": "karma start --single-run",
    "tdd": "npm test -- --watch",
    "tdd:browser": "karma start",
    "postinstall": "npm run build"
  },
  "repository": "<%= username %>/<%= moduleName %>",
  "keywords": [
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "isparta": "^3.1.0",
    "karma": "^0.13.14",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.18",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.3.3",
    "phantomjs-polyfill": "0.0.1",
    "pre-commit": "^1.1.2",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "pre-commit": [
    "test",
    "test:browser",
    "build"
  ],
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT",
  "dependencies": {}
}
